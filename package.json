    "private": false,
    "repository": {
        "type": "git",
        "url": "https://github.com/opengraphica/opengraphica/"
    },
    "scripts": {
        "dev": "webpack serve --config ./webpack.config.dev.js",
        "build": "npm run build:website",
        "build:css": "node ./src/css/build.js",
        "build:library": "webpack --mode=production --config ./webpack.config.library.js",
        "build:website": "webpack --mode=production --config ./webpack.config.website.js",
        "build:website:inspect": "node --inspect webpack --config ./webpack.config.website.js --stats verbose",
        "serve": "npm run dev",
        "translate": "node ./scripts/translate.js"
    },
